@model UltimoIntento.Controllers.Clientes

@{
    ViewBag.Title = "Details";
}

<h2>Detalle</h2>

<div>
    <h4>Clientes</h4>
    <hr />
    <dl class="dl-horizontal" align="center">
        <dt>
            Nombre Comercial
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Nombre_Comercial)
        </dd>

        <dt>
            Cedula
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Cedula)
        </dd>

        <dt>
            Estado
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Estado)
        </dd>

    </dl>
</div>
<p align="right">
    @Html.ActionLink("Editar", "Edit", new { id = Model.IdCliente }) |
    @Html.ActionLink("Regresar a la lista", "Index")
</p>
